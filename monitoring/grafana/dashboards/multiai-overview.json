{
  "dashboard": {
    "id": null,
    "title": "MULTIAI V5 - Production Overview",
    "tags": [
      "multiai",
      "production"
    ],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "API Request Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(api_requests_total[5m])",
            "legendFormat": "{{method}} {{status}}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Error Rate",
        "type": "gauge",
        "targets": [
          {
            "expr": "rate(api_requests_total{status=~'5..'}[5m]) / rate(api_requests_total[5m])",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Ledger Operations",
        "type": "barchart",
        "targets": [
          {
            "expr": "rate(ledger_writes_total[5m])",
            "legendFormat": "writes",
            "refId": "A"
          },
          {
            "expr": "rate(ledger_verifications_total[5m])",
            "legendFormat": "verifications",
            "refId": "B"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Response Time (95th %ile)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(request_duration_seconds_bucket[5m]))",
            "legendFormat": "{{endpoint}}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 5,
        "title": "Active Sprints",
        "type": "stat",
        "targets": [
          {
            "expr": "active_sprints",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 6,
        "title": "Budget Usage",
        "type": "gauge",
        "targets": [
          {
            "expr": "budget_usage_ratio",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 6,
          "y": 16
        }
      },
      {
        "id": 7,
        "title": "LLM Provider Usage",
        "type": "piechart",
        "targets": [
          {
            "expr": "rate(llm_calls_total[5m])",
            "legendFormat": "{{provider}}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 22
        }
      }
    ],
    "time": {
      "from": "now-6h",
      "to": "now"
    }
  }
}